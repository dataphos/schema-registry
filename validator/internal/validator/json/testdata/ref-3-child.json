{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "The Address Schema",
  "description": "Reusable Address schema",
  "default": {},
  "additionalProperties": false,
  "required": [
    "street"
  ],
  "properties": {
    "street": {
      "type": "string",
      "title": "Street name"
    },
    "city": {
      "type": "string",
      "title": "City name",
      "default": ""
    }
  }
}
